program DBMigrations;

{$APPTYPE CONSOLE}
{$R *.res}

uses
  System.SysUtils;

begin
  if ParamCount > 0 then
  begin

    if ParamStr(1).ToLower = '--help' then
    begin
      Writeln('--init  To start manager DBMigrations');
      Writeln('--new To Create migration file');
      Writeln('--run Apply Migrations');
      Exit;
    end;

  end;

end.
