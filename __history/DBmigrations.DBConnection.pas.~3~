unit DBmigrations.DBConnection;

interface

uses
  DBmigrations.Interfaces;

type
  TDBConnection = class(TInterfacedObject, IDBConnection)
  private
    FDrive: String;
    FDriveName: String;
    FDatabase: String;

  public
  property DriveID: String read FDrive write FDrive;
  property DriveName: String read FDriveName write FDriveName;
  property Database: String read FDatabase write FDatabase;

  end;

implementation

end.
